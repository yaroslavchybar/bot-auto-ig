{
    "compilerOptions": {
        "target": "ESNext",
        "lib": [
            "ES2021",
            "DOM",
            "DOM.Iterable"
        ],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true
    },
    "include": [
        "./**/*"
    ],
    "exclude": [
        "_generated"
    ]
}
